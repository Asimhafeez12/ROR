    "prop-types": "^15.6.1",
    "react": "^16.4.2",
    "react-actioncable-provider": "^1.0.3",
    "react-country-region-selector": "^1.3.0",
    "react-country-select": "^1.0.5",
    "react-device-detect": "^1.6.1",
    "react-dom": "^16.4.2",
    "react-icons-kit": "^1.1.5",
