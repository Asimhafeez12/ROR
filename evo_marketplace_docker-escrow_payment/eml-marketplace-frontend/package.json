    "react-quill": "^1.2.7",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.2.2",
    "react-router-redux": "^5.0.0-alpha.9",
    "react-scripts": "1.1.4",
    "react-scroll-parallax": "^1.3.5",
    "react-svg": "^5.0.6",
    "react-tag-input": "^5.0.2",
    "redux": "^4.0.0",
